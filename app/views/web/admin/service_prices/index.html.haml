%table.table
  %tr.table-info
    %th= ServicePrice.human_attribute_name (:id)
    %th= t('control')
  - @service_prices.each do |service_price|
    %tr.bg-success
      %td=service_price.id
      /%td=link_to t('.view profile'), admin_company_path(company)
.btn.btn-light
  = link_to t('.back'), admin_root_path, :class => "btn btn-info"
/.btn.btn-light
/  = link_to t('.create new'), new_admin_company_path, :class => "btn btn-info"


