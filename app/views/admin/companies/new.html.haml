= simple_form_for @company, url: admin_companies_path do |f|
  = f.input :name
  = f.input :email
  = f.input :password
  = f.input :requisites
  = f.input :description
  = f.input :rating
  = f.input :website
  = f.input :phone_number
  = f.button :submit, class: 'btn btn-info'
  = link_to t('.cancel'), admin_companies_path, :class => "btn btn-info"
